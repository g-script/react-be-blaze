{
  "name": "react-be-blaze--demo",
  "version": "1.0.0",
  "description": "react-be-blaze website source code",
  "scripts": {
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "lint": "eslint webpack.config.js ./src --fix"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --inline",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run lint && webpack --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react",
    "blaze",
    "blazecss",
    "react-be-blaze"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:g-script/react-be-blaze.git"
  },
  "author": "Nicolas Goudry <goudry.nicolas@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "better-npm-run": "0.0.13",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.24.0",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2",
    "webpack-validator": "^2.2.9"
